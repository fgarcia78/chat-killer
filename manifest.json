{
  "name": "Chat killer (edx's cs-169.2x)",
  "version": "1.0",
  "description": "A extension to kill the chat iframe in edx.org's cs-169.2x course.",
  "permissions": [
    "activeTab", "https://courses.edx.org/courses/BerkeleyX/CS-169.2x/*"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Chat killer (edx.org's cs-169.2x)",
    "default_icon": "icon.png"
  },
  "manifest_version": 2
}
